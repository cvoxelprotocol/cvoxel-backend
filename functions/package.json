    "@ceramicnetwork/stream-tile": "^2.4.3",
    "@didtools/pkh-ethereum": "^0.0.2",
    "@glazed/did-datastore": "^0.3.2",
    "@glazed/types": "^0.2.0",
    "@metamask/eth-sig-util": "^5.0.0",
    "@sendgrid/mail": "^7.6.0",
    "@types/node-fetch": "^2.6.2",
    "bignumber.js": "^9.0.2",
    "caip": "^1.1.0",
    "did-session": "^1.0.0",
    "dids": "^3.3.0",
    "eslint-config-prettier": "^8.5.0",
    "ethers": "^5.7.1",
    "firebase-admin": "^11.0.1",
    "firebase-functions": "^3.24.0",
    "firebase-functions-test": "^2.4.0",
    "key-did-provider-ed25519": "^2.0.1",
    "key-did-resolver": "^2.1.1",
    "node-fetch": "^3.2.10",
    "prettier": "^2.6.0",
    "uint8arrays": "3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
