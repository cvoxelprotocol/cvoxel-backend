    "@metamask/eth-sig-util": "^5.0.0",
    "@sendgrid/mail": "^7.6.0",
    "@types/node-fetch": "^2.6.2",
    "bignumber.js": "^9.0.2",
    "did-session": "^0.1.1",
    "dids": "^3.3.0",
    "eslint-config-prettier": "^8.5.0",
    "ethers": "^5.6.2",
    "firebase-admin": "^11.0.1",
    "firebase-functions": "^3.24.0",
    "firebase-functions-test": "^2.4.0",
